{
  "metadata": {
    "name": "MITRE Engage Framework Data",
    "version": "1.0",
    "description": "Engage activities mapped to ATT&CK techniques for cyber deception",
    "source": "https://engage.mitre.org"
  },
  "engage_activities": {
    "EAC0001": {
      "id": "EAC0001",
      "name": "Honeypots",
      "tactic": "Expose",
      "description": "Deploy a system with no authorized user activity that is intended to be targeted by adversaries.",
      "implementation": {
        "action_type": "deploy_honeypot",
        "parameters": ["honeypot_type", "network_segment", "services", "interaction_level"]
      }
    },
    "EAC0002": {
      "id": "EAC0002",
      "name": "Honeytokens",
      "tactic": "Expose",
      "description": "Deploy deceptive data or credentials intended to trigger alerts when accessed.",
      "implementation": {
        "action_type": "deploy_honeytoken",
        "parameters": ["token_type", "placement", "appearance", "alert_on_access"]
      }
    },
    "EAC0003": {
      "id": "EAC0003",
      "name": "System Activity Monitoring",
      "tactic": "Detect",
      "description": "Monitor system activity to detect adversary behavior.",
      "implementation": {
        "action_type": "enable_monitoring",
        "parameters": ["monitoring_type", "target_systems", "alert_threshold"]
      }
    },
    "EAC0004": {
      "id": "EAC0004",
      "name": "Network Monitoring",
      "tactic": "Detect",
      "description": "Monitor network traffic to detect adversary behavior.",
      "implementation": {
        "action_type": "enable_network_monitoring",
        "parameters": ["network_segment", "capture_type", "analysis_rules"]
      }
    },
    "EAC0005": {
      "id": "EAC0005",
      "name": "Lures",
      "tactic": "Affect",
      "description": "Deploy deceptive artifacts to entice adversary interaction.",
      "implementation": {
        "action_type": "deploy_lure",
        "parameters": ["lure_type", "placement", "target_behavior", "points_to"]
      }
    },
    "EAC0006": {
      "id": "EAC0006",
      "name": "Decoy Accounts",
      "tactic": "Expose",
      "description": "Create user accounts with no legitimate purpose for adversaries to discover and target.",
      "implementation": {
        "action_type": "deploy_decoy_account",
        "parameters": ["account_type", "privilege_level", "domain", "honeypot_target"]
      }
    },
    "EAC0007": {
      "id": "EAC0007",
      "name": "Decoy Credentials",
      "tactic": "Expose",
      "description": "Deploy fake credentials for adversaries to discover and attempt to use.",
      "implementation": {
        "action_type": "deploy_decoy_credentials",
        "parameters": ["credential_type", "placement", "target_asset", "honeypot_target"]
      }
    },
    "EAC0008": {
      "id": "EAC0008",
      "name": "Decoy Content",
      "tactic": "Affect",
      "description": "Deploy deceptive files or data that appear valuable to adversaries.",
      "implementation": {
        "action_type": "deploy_decoy_content",
        "parameters": ["content_type", "placement", "appearance", "tracking_enabled"]
      }
    },
    "EAC0009": {
      "id": "EAC0009",
      "name": "Decoy Networks",
      "tactic": "Expose",
      "description": "Deploy network segments that appear legitimate but are entirely monitored deception.",
      "implementation": {
        "action_type": "deploy_decoy_network",
        "parameters": ["network_topology", "services", "connectivity", "monitoring_level"]
      }
    },
    "EAC0010": {
      "id": "EAC0010",
      "name": "Decoy Systems",
      "tactic": "Expose",
      "description": "Deploy systems that appear to be legitimate production assets but are monitored decoys.",
      "implementation": {
        "action_type": "deploy_decoy_system",
        "parameters": ["system_type", "os", "services", "network_location"]
      }
    },
    "EAC0011": {
      "id": "EAC0011",
      "name": "Pocket Litter",
      "tactic": "Affect",
      "description": "Deploy small deceptive artifacts that add believability to an environment.",
      "implementation": {
        "action_type": "deploy_pocket_litter",
        "parameters": ["artifact_type", "placement", "content", "references"]
      }
    },
    "EAC0012": {
      "id": "EAC0012",
      "name": "Burn-in",
      "tactic": "Prepare",
      "description": "Create realistic activity history to make deception environments believable.",
      "implementation": {
        "action_type": "create_burn_in",
        "parameters": ["activity_type", "duration", "target_systems", "patterns"]
      }
    },
    "EAC0013": {
      "id": "EAC0013",
      "name": "Isolation",
      "tactic": "Contain",
      "description": "Isolate adversary activity to limit damage and enable observation.",
      "implementation": {
        "action_type": "isolate_system",
        "parameters": ["isolation_type", "target", "allow_outbound", "monitoring"]
      }
    },
    "EAC0014": {
      "id": "EAC0014",
      "name": "Network Diversity",
      "tactic": "Protect",
      "description": "Deploy network configurations that increase adversary uncertainty.",
      "implementation": {
        "action_type": "configure_network_diversity",
        "parameters": ["diversity_type", "segments", "rotation_policy"]
      }
    },
    "EAC0015": {
      "id": "EAC0015",
      "name": "Information Manipulation",
      "tactic": "Affect",
      "description": "Manipulate information to mislead adversaries about the environment.",
      "implementation": {
        "action_type": "manipulate_information",
        "parameters": ["manipulation_type", "target_data", "false_info", "scope"]
      }
    },
    "EAC0016": {
      "id": "EAC0016",
      "name": "Network Manipulation",
      "tactic": "Affect",
      "description": "Manipulate network traffic to affect adversary operations.",
      "implementation": {
        "action_type": "manipulate_network",
        "parameters": ["manipulation_type", "target_traffic", "effect", "duration"]
      }
    },
    "EAC0017": {
      "id": "EAC0017",
      "name": "Software Manipulation",
      "tactic": "Affect",
      "description": "Alter software behavior to mislead or detect adversaries.",
      "implementation": {
        "action_type": "manipulate_software",
        "parameters": ["target_software", "manipulation_type", "trigger_conditions"]
      }
    },
    "EAC0018": {
      "id": "EAC0018",
      "name": "Security Controls",
      "tactic": "Protect",
      "description": "Deploy or modify security controls in response to adversary activity.",
      "implementation": {
        "action_type": "deploy_security_control",
        "parameters": ["control_type", "target", "configuration", "monitoring"]
      }
    },
    "EAC0019": {
      "id": "EAC0019",
      "name": "Persona Creation",
      "tactic": "Prepare",
      "description": "Create fake personas that can interact with adversaries.",
      "implementation": {
        "action_type": "create_persona",
        "parameters": ["persona_type", "identity_details", "communication_channels", "backstory"]
      }
    },
    "EAC0020": {
      "id": "EAC0020",
      "name": "Artifact Diversity",
      "tactic": "Prepare",
      "description": "Use diverse artifacts to increase adversary uncertainty.",
      "implementation": {
        "action_type": "deploy_artifact_diversity",
        "parameters": ["artifact_types", "distribution", "variation_rules"]
      }
    }
  },
